/* vw 계산 기준 1920px */

@import url(//cdn.design-factory.co.kr/_font/gowunbatang/fonts.css);
@import url(//cdn.design-factory.co.kr/_font/plusjakartasans/fonts.css);
@import url(//cdn.design-factory.co.kr/_font/pretendard/fonts.css);

* {font-style:normal; line-height:normal; font-family:"Pretendard", sans-serif; word-break:keep-all; box-sizing:border-box; -moz-box-sizing:border-box; -webkit-box-sizing:border-box;}

/*
	font-family:"Gowun Batang", sans-serif;
	font-family:"Plus Jakarta Sans", sans-serif;
	font-family:"Pretendard", sans-serif;
*/

html {-webkit-text-size-adjust:none; -moz-text-size-adjust:none; -ms-text-size-adjust:none; -o-text-size-adjust:none;}
body {width:100%; margin:0 auto; padding:0; font-size:13px; line-height:normal; overflow-y:scroll;}
body.hidden {height:100%; min-height:100%; overflow:hidden !important; touch-action:none;}
img {border:none; padding:0; margin:0; outline:none;}
a {border:none; padding:0; margin:0; outline:none; text-decoration:none;}
a:active {color:#666666; text-decoration:none;}
a:link {color:#666666; text-decoration:none;}
a:visited {color:#666666; text-decoration:none;}
a:hover {color:#666666; text-decoration:none;}
ul {list-style:none; padding:0; margin:0;}
ul:after {content:""; display:block; clear:both;}
ul > li {display:block;}

img.fx{max-width:100%;}

td {
	box-sizing:border-box;
	-moz-box-sizing:border-box;
	-webkit-box-sizing:border-box;
}

input, select, textarea {
	box-sizing:border-box;
	-moz-box-sizing:border-box;
	-webkit-box-sizing:border-box;
	margin:0; 
	padding:0; 
	outline:none;
	font-size:min(18px, 0.94vw);
	color:rgba(55,55,55,0.50);
	font-weight:300;
}

input::placeholder {color:rgba(55,55,55,0.50); opacity:1;}
input::-webkit-input-placeholder {color:rgba(55,55,55,0.50);}
input:-ms-input-placeholder {color:rgba(55,55,55,0.50);}
input:-moz-input-placeholder {color:rgba(55,55,55,0.50);}

textarea {overflow-y:scroll; resize:none;}

textarea::placeholder {color:rgba(55,55,55,0.50); opacity:1;}
textarea::-webkit-input-placeholder {color:rgba(55,55,55,0.50);}
textarea:-ms-input-placeholder {color:rgba(55,55,55,0.50);}
textarea:-moz-input-placeholder {color:rgba(55,55,55,0.50);}

input[type=text], input[type=password] ,input[type=tel], input[type=file], input[type=image], input[type=button], input[type=submit], textarea {
	border-radius:0; 
	-moz-border-radius:0; 
	-webkit-border-radius:0;
	appearance:none;
	-moz-appearance:none;
	-webkit-appearance:none;
	width:100%;
}

select {
	border-radius:0; 
	-moz-border-radius:0; 
	-webkit-border-radius:0;
	appearance:none;
	-moz-appearance:none;
	-webkit-appearance:none;
	background-color:#ffffff;
	background-image:url("/kr/img/sub/select_arrow.svg");
	background-repeat:no-repeat;
	background-size:min(18px, 0.94vw); 
	background-position:right center;
	width:100%;
	padding:0 min(28px, 1.46vw) 0 0;
}

select::-ms-expand {display:none;}

.w_con {}
.m_con {display:none;}

.w_img {display:inline-block; width:100%;}
.m_img {display:none;}

.w_br {}
.m_br {display:none;}

.w_bar {}
.m_bar {display:none;}

/*번호목록*/
.number_list_con {text-align:center;}
.number_list_con > .contents_con {display:inline-block;}
.number_list_con > .contents_con:after {content:""; display:block; clear:both;}
.number_list_con > .contents_con a {display:inline-block;}
.number_list_con > .contents_con .btn_con {display:inline-block; vertical-align:middle;}
.number_list_con > .contents_con .btn_con a {margin:0 0 0 min(12px, 0.63vw);}
.number_list_con > .contents_con .btn_con a img {width:auto; max-width:none; height:min(16px, 0.83vw);}
.number_list_con > .contents_con .btn_con a:first-child {margin:0;}
.number_list_con > .contents_con .list_con {display:inline-block; margin:0 min(17px, 0.89vw); position:relative; top:0; left:0; vertical-align:middle;}
.number_list_con > .contents_con .list_con:after {content:""; display:block; clear:both;}
.number_list_con > .contents_con .list_con .list_a {margin:0 0 0 min(26px, 1.35vw); font-size:min(18px, 0.94vw); color:rgba(5,5,5,0.5); font-weight:700; line-height:110%; letter-spacing:max(-0.44px, -0.02vw);}
.number_list_con > .contents_con .list_con .list_a:first-child {margin:0; color:#050505;}
.number_list_con > .contents_con .list_con .bar {width:min(2px, 0.1vw); height:min(20px, 1.04vw); margin:0 auto; background-color:rgba(5,5,5,0.5); position:absolute; top:50%; left:0; right:0; z-index:10; transform:translateY(-50%);}

#wrap {width:100%; min-width:280px; margin:0 auto;}

